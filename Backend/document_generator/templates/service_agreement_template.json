{
  "document_type": "Software as a Service (SaaS) Agreement",
  "template_version": "1.0",
  "structure": {
    "title": {
      "type": "heading",
      "level": 1,
      "content": "{{document_type}}"
    },
    "header_section": {
      "type": "header_details",
      "content": [
        {
          "label": "Effective Date:",
          "value": "{{effective_date}}"
        }
      ]
    },
    "parties": {
      "type": "section",
      "content": "Parties:",
      "subsections": {
        "provider": {
          "type": "party_details",
          "label": "Provider:",
          "fields": {
            "company_name": "{{provider.company_name}}",
            "address": "{{provider.address}}",
            "email": "{{provider.email}}"
          }
        },
        "customer": {
          "type": "party_details",
          "label": "Customer:",
          "fields": {
            "company_name": "{{customer.company_name}}",
            "address": "{{customer.address}}",
            "email": "{{customer.email}}"
          }
        }
      }
    },
    "sections": [
      {
        "number": "1",
        "title": "Services",
        "type": "services_section",
        "content": "{{provider.company_name}} agrees to provide {{service_description}} under the terms of this Agreement."
      },
      {
        "number": "2",
        "title": "Subscription & Fees",
        "type": "subscription_section",
        "content": "The Customer shall pay {{subscription_fee}} for access to the {{service_plan}}. {{payment_terms}}"
      },
      {
        "number": "3",
        "title": "Service Level Agreement (SLA)",
        "type": "sla_section",
        "content": "{{provider.company_name}} guarantees {{uptime_guarantee}} uptime monthly. {{maintenance_notice}}"
      },
      {
        "number": "4",
        "title": "Data Ownership",
        "type": "data_section",
        "content": "{{data_ownership_clause}}"
      },
      {
        "number": "5",
        "title": "Confidentiality",
        "type": "confidentiality_section",
        "content": "{{confidentiality_clause}}"
      },
      {
        "number": "6",
        "title": "Term & Termination",
        "type": "term_section",
        "content": "{{term_and_termination_clause}}"
      },
      {
        "number": "7",
        "title": "Limitation of Liability",
        "type": "liability_section",
        "content": "{{liability_limitation_clause}}"
      },
      {
        "number": "8",
        "title": "{{additional_sections_title}}",
        "type": "additional_section",
        "content": "{{additional_sections_content}}"
      },
      {
        "number": "9",
        "title": "Governing Law",
        "type": "governing_law_section",
        "content": "{{governing_law_clause}}"
      }
    ],
    "signature_section": {
      "type": "signature_block",
      "content": "IN WITNESS WHEREOF, the parties have executed this Agreement.",
      "signatures": {
        "provider": {
          "label": "{{provider.company_name}}",
          "fields": {
            "name": "Name: {{provider.signatory_name}}",
            "title": "Title: {{provider.signatory_title}}"
          }
        },
        "customer": {
          "label": "{{customer.company_name}}",
          "fields": {
            "name": "Name: {{customer.signatory_name}}",
            "title": "Title: {{customer.signatory_title}}"
          }
        }
      }
    }
  },
