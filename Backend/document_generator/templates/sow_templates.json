{
  "document_type": "Statement of Work (SOW)",
  "template_version": "1.0",
  "structure": {
    "title": {
      "type": "heading",
      "level": 1,
      "content": "Statement of Work (SOW)"
    },
    "header_section": {
      "type": "header_details",
      "content": [
        {
          "label": "Date:",
          "value": "{{document_date}}"
        },
        {
          "label": "Project Title:",
          "value": "{{project_title}}"
        }
      ]
    },
    "parties": {
      "type": "section",
      "content": "",
      "subsections": {
        "client": {
          "type": "party_details",
          "label": "Client:",
          "fields": {
            "company_name": "{{client.company_name}}",
            "address": "{{client.address}}",
            "email": "{{client.email}}"
          }
        },
        "service_provider": {
          "type": "party_details",
          "label": "Service Provider:",
          "fields": {
            "name": "{{service_provider.name}}",
            "email": "{{service_provider.email}}",
            "address": "{{service_provider.address}}"
          }
        }
      }
    },
    "sections": [
      {
        "number": "1",
        "title": "Parties",
        "type": "parties_reference",
        "content": "This Statement of Work is entered into between the Client and Service Provider as detailed above."
      },
      {
        "number": "2",
        "title": "{{objective_section_title}}",
        "type": "objective_section",
        "content": "{{project_objective}}"
      },
      {
        "number": "3",
        "title": "Scope of Work",
        "type": "scope_section",
        "content": "The Service Provider shall deliver the following services:",
        "subsections": {
          "type": "bullet_list",
          "items": "{{scope_of_work}}"
        }
      },
      {
        "number": "4",
        "title": "Timeline",
        "type": "timeline_section",
        "content": "Project Timeline Details:",
        "subsections": {
          "type": "timeline_details",
          "items": [
            {
              "label": "Project Duration:",
              "value": "{{timeline.duration}}"
            },
            {
              "label": "Start Date:",
              "value": "{{timeline.start_date}}"
            },
            {
              "label": "End Date:",
              "value": "{{timeline.end_date}}"
            }
          ]
        }
      },
      {
        "number": "5",
        "title": "Deliverables",
        "type": "deliverables_section",
        "content": "The following deliverables will be provided upon project completion:",
        "subsections": {
          "type": "bullet_list",
          "items": "{{deliverables}}"
        }
      },
      {
        "number": "6",
        "title": "Payment Terms",
        "type": "payment_section",
        "content": "Payment Structure and Terms:",
        "subsections": {
          "type": "payment_details",
          "items": [
            {
              "label": "Total Project Fee:",
              "value": "{{payment.total_fee}}"
            },
            {
              "label": "Payment Schedule:",
              "value": "{{payment.schedule}}"
            },
            {
              "label": "Payment Method:",
              "value": "{{payment.method}}"
            }
          ]
        }
      },
      {
        "number": "7",
        "title": "Assumptions",
        "type": "assumptions_section",
        "content": "This project is based on the following assumptions:",
        "subsections": {
          "type": "bullet_list",
          "items": "{{assumptions}}"
        }
      },
      {
        "number": "8",
        "title": "{{additional_terms_title}}",
        "type": "additional_terms_section",
        "content": "{{additional_terms_content}}"
      },
      {
        "number": "9",
        "title": "Termination",
        "type": "termination_section",
        "content": "{{termination_clause}}"
      },
      {
        "number": "10",
        "title": "{{final_section_title}}",
        "type": "approval_section",
        "content": "{{approval_clause}}"
      }
    ],
    "signature_section": {
      "type": "signature_block",
      "content": "",
      "signatures": {
        "client": {
          "label": "{{client.company_name}}",
          "fields": {
            "name": "Name: {{client.signatory_name}}",
            "title": "Title: {{client.signatory_title}}"
          }
        },
        "service_provider": {
          "label": "{{service_provider.name}}",
          "fields": {
            "name": "Name: {{service_provider.signatory_name}}",
            "title": "Title: {{service_provider.signatory_title}}"
          }
        }
      }
    }
  },
