  "formatting": {
    "page_margins": {
      "top": "1 inch",
      "bottom": "1 inch",
      "left": "1 inch",
      "right": "1 inch"
    },
    "font": {
      "family": "Arial",
      "size": "12pt"
    },
    "line_spacing": "1.15",
    "section_spacing": "12pt",
    "numbering": {
      "sections": "numeric",
      "bullets": "bullet_points"
    },
    "page_numbering": true
  }
